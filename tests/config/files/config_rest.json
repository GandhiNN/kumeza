{
    "runtime_environment": {
        "id": "aws_tenant",
        "provider": "aws",
        "service": "glue",
        "region": "ap-southeast-3",
        "env": "dev"
    },
    "source_system": {
        "id": "custdb",
        "type": "timeseries",
        "database_engine": "influx",
        "database_instance": "dev",
        "authentication_type": "ntlm",
        "hostname": "custdb.mssql.example",
        "domain": "example.com",
        "port": 443,
        "physical_location": "jkt"
    },
    "integration": {
        "engine": "rest",
        "driver": null,
        "fetchsize": 1000,
        "chunksize": 1000000
    },
    "rest": {
        "header": {"Accept": "application/csv", "Content-Type": "multipart/form-data"},
        "params": {"chunked": true, "chunk_size": 2000}
    },
    "credentials": {
        "username": "some_username",
        "provider": "hashicorp_vault",
        "url": "https://hashicorp-vault.dev.com:8200",
        "verify_ssl": false,
        "namespace": "some_vault_namespace",
        "secret_name": "hcv-approle-dev",
        "mount_point": "static-secret",
        "path": "data/custdb"
    },
    "metadata": [
        {   
            "metadata_type": "ingestion_status",
            "sink_target": "dynamodb",
            "table_name": "ingestion-status-dev",
            "partition_key": "ingestor_name",
            "sort_key": "execution_time"
        },
        {
            "metadata_type": "schema_registrar",
            "sink_target": "dynamodb",
            "table_name": "schema-status-dev",
            "partition_key": "ingestor_name",
            "sort_key": "execution_time"
        }
    ],
    "sinks": [
        {
            "sink_type": "raw",
            "sink_targets": [
                {
                    "id": "staging",
                    "target": "s3",
                    "file_format": "parquet",
                    "path": "staging-bucket-dev"
                },
                {
                    "id": "silver",
                    "target": "s3",
                    "file_format": "parquet",
                    "path": "silver-bucket-dev"
                }
            ]
        }, 
        {
            "sink_type": "schema",
            "sink_targets": [
                {
                    "id": "raw_schema",
                    "sink_type": "schema",
                    "target": "s3",
                    "file_format": "json",
                    "path": "schema-bucket-dev"
                }
            ]
        }
    ],
    "data_assets": [
        {
            "id": "group_1",
            "database_name": "master",
            "assets": [
                {
                    "asset_name": "CUSTOMER_ID",
                    "asset_type": "table",
                    "database_schema": "dbo",
                    "query_type": "standard",
                    "incremental": true,
                    "incremental_column": "updated_at",
                    "reload": false,
                    "partition_columns": [],
                    "columns_to_anonymize": [],
                    "custom_query": null,
                    "custom_schema": {},
                    "cast_timestamp_columns_to_string": false
                },
                {
                    "asset_name": "CUSTOMER_NAME",
                    "asset_type": "table",
                    "database_schema": "dbo",
                    "query_type": "standard",
                    "incremental": false,
                    "incremental_column": "",
                    "reload": false,
                    "partition_columns": [],
                    "columns_to_anonymize": [],
                    "custom_query": null,
                    "custom_schema": {},
                    "cast_timestamp_columns_to_string": false
                }
            ]
        },
        {
            "id": "group_2",
            "database_name": "master",
            "assets": [
                {
                    "asset_name": "ADDRESS",
                    "asset_type": "table",
                    "database_schema": "dbo",
                    "query_type": "standard",
                    "incremental": false,
                    "incremental_column": "created_at",
                    "reload": false,
                    "partition_columns": [],
                    "columns_to_anonymize": [],
                    "custom_query": null,
                    "custom_schema": {},
                    "cast_timestamp_columns_to_string": false
                }
            ]
        }
    ]
}